// RUN: cxx-comp %s -res-dump 2>&1 | FileCheck %s --match-full-lines
fn pass(): number {
    let x: number = 2.0;
    return x * 10.0;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator: '*'
// CHECK-NEXT:       | value: 20
// CHECK-NEXT:         ResolvedDeclRefExpr: @({{.*}}) x
// CHECK-NEXT:         ResolvedNumberLiteral: '10'

fn pass2(): number {
    let x: number = 2.0;
    return x;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedDeclRefExpr: @({{.*}}) x
// CHECK-NEXT:       | value: 2

fn err(): number {
    let x: number;
    x = 2.0;
    return x * 10.0;
}
// CHECK:     ResolvedReturnStmt
// CHECK-NEXT:       ResolvedBinaryOperator: '*'
// CHECK-NEXT:         ResolvedDeclRefExpr: @({{.*}}) x
// CHECK-NEXT:         ResolvedNumberLiteral: '10'

fn err2(): number {
    var x: number = 2.0;
    return x * 10.0;
}
// CHECK:    ResolvedReturnStmt
// CHECK-NEXT:      ResolvedBinaryOperator: '*'
// CHECK-NEXT:        ResolvedDeclRefExpr: @({{.*}}) x
// CHECK-NEXT:        ResolvedNumberLiteral: '10'

fn main(): void {}
